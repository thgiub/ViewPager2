<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <ImageView
        android:id="@+id/img_album"
        android:layout_width="0dp"
        android:layout_height="50dp"
        app:layout_constraintEnd_toStartOf="@+id/tv_music_list"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintHorizontal_weight="0.1"
        >
    </ImageView>
    <TextView
        android:id="@+id/tv_music_list"
        android:layout_width="0dp"
        android:layout_height="50dp"
        app:layout_constraintEnd_toStartOf="@+id/iv_star_list"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintHorizontal_weight="1"
        app:layout_constraintStart_toEndOf="@+id/img_album"/>
    <ImageView
        android:id="@+id/iv_star_list"
        android:layout_width="0dp"
        android:layout_height="50dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tv_music_list"
        app:layout_constraintEnd_toStartOf="@+id/tv_item_time"
        app:layout_constraintHorizontal_weight="0.1"
        >
    </ImageView>
    <TextView
        android:id="@+id/tv_item_time"
        android:layout_width="0dp"
        android:layout_height="50dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintHorizontal_weight="0.1"
        app:layout_constraintEnd_toStartOf="@id/iv_item_music_settings"
        app:layout_constraintStart_toEndOf="@+id/img_album"/>
    <ImageView
        android:id="@+id/iv_item_music_settings"
        android:layout_width="0dp"
        android:layout_height="50dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tv_item_time"
        app:layout_constraintHorizontal_weight="0.1"
        >
    </ImageView>

</androidx.constraintlayout.widget.ConstraintLayout>